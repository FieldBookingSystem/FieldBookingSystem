  <section>
  <!-- The contents of this card links to the gallery page using the gallery ID -->
  {{{body}}}
  {{#each fields as |field| }}
  <div class="card pure-u-md-15-24">
    <div class="fields">
            <h2 class="pure-u-md-15-24">{{field.name}}</h2>   
            <p class="pure-u-md-15-24"> {{field.address}}</p>  
            <p class="pure-u-md-15-24"> {{field.id}}</p> 
            <button> Book This Field </button>    
    </div>
</div>
    {{/each}}
</section>





